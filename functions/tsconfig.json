{
	"compilerOptions": {
		"target": "es6",
		"lib": ["es6", "dom"],
		"module": "commonjs",
		"types": ["reflect-metadata"],
		"experimentalDecorators":true,
		"emitDecoratorMetadata": true,
		"outDir": "./",
		"noImplicitAny": true,
		"strictNullChecks": false
	},
	"include": [
		"src/**/*.ts"
	],
	"exclude": [
		"node_modules"
	]
}